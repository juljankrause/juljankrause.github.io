	month = feb,
	year = {2019},
	keywords = {H4, O30, O38, public R\&D investment, R\&D policy, rate of return to public R\&D investment, vector-error-correction model},
	pages = {1--17},
	file = {Full Text PDF:C\:\\Users\\jk2u1\\Zotero\\storage\\EQJGMHCK\\Soete et al. - 2019 - The productivity effect of public R&D in the Nethe.pdf:application/pdf;Snapshot:C\:\\Users\\jk2u1\\Zotero\\storage\\GEJ3ANSN\\10438599.2019.html:text/html},
