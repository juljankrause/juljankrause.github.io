	file = {ScienceDirect Full Text PDF:C\:\\Users\\jk2u1\\Zotero\\storage\\7PQZWKS4\\Marmor et al. - 1979 - The technology assessment and forecast program of .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\jk2u1\\Zotero\\storage\\FBKALECZ\\0172219079900061.html:text/html},
}

