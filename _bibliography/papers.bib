	journal = {Sustainability},
	author = {Ji, Yakun and Yu, Xiang and Sun, Minghan and Zhang, Ben},
	month = jan,
	year = {2022},
	note = {Number: 3
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {ERGM, exponential random graph models, open innovation, patent citation, social network},
	pages = {1618},
	file = {Ji et al. - 2022 - Exploring the Evolution and Determinants of Open I.pdf:C\:\\Users\\jk2u1\\Zotero\\storage\\WFRGDM2L\\Ji et al. - 2022 - Exploring the Evolution and Determinants of Open I.pdf:application/pdf},
