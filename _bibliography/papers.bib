@techreport{artRISINGINFLUENCECHINA2020,
	address = {Brussels},
	type = {Issues paper},
	title = {{THE} {RISING} {INFLUENCE} {OF} {CHINA} {IN} {INTERGOVERNMENTAL} {ORGANISATIONS} {AND} {STANDARDISATION} {BODIES}},
	url = {https://www.consilium.europa.eu/media/54626/the-rising-influence-of-china-in-intergovernmental-organisations-and-standardisation-bodies-17-december-2020.pdf},
	abstract = {Global influence can be exercised in a number of ways. For the EU, that has traditionally included a strong
commitment to multilateral institutions and standardisation bodies, and the willingness to use them to
promote its system of values, but also to defend its own interests. This is in addition to the so-called
"Brussels effect" which is an indirect way in which the EU, through the size of its market, has become a
global standards setter.
But China is increasingly seeing the value of these institutions and bodies as a way of pursuing its own
interests. This paper describes how this is happening. It does so by looking at how influence on decisionmaking in the United Nations and its agencies is exercised, whether in terms of budget, coalition building
or leadership positions, and at the level of political investment in international standardisation
organisations.
The long-term impact of this trend is not only about who manages to be most effective at using
multilateral institutions for their own interests, but also how the nature of these institutions could change
over time.
The paper includes a look at the possible impact on the issue of the attitude and policy of the new Biden
administration.},
	urldate = {2022-08-21},
	institution = {Council of the European Union: General Secretariat},
	author = {ART},
