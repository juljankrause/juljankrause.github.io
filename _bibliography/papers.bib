@techreport{bikardStandingShouldersMale2022,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {Standing on the {Shoulders} of ({Male}) {Giants}: {Gender} {Inequality} and the {Technological} {Impact} of {Scientific} {Ideas}},
	shorttitle = {Standing on the {Shoulders} of ({Male}) {Giants}},
	url = {https://papers.ssrn.com/abstract=4059813},
	abstract = {We argue that gender inequality in science and technology means that ideas are less likely to be built upon if their author is a woman versus a man. Testing this empirically is challenging because men and women tend to work on different ideas whose potential is largely unobservable. To address this challenge, we exploit the occurrence of simultaneous discoveries in science – i.e., instances when a man and a woman have published the same idea around the same time – and track the citations to their subsequent publications in patented inventions. We find that scientific publications receive fewer patent citations, that is, they have a lower technological impact, when their main author is a woman. This gap is not driven by women’s lower propensity to produce patented inventions based on their own ideas, but rather by other inventors’ lower likelihood to build on those ideas. Additional analysis suggests that supply-side factors alone, such as the greater saliency of men’s work, are unlikely to drive our results. Rather, inventors seem to pay more attention to men’s ideas. Our research highlights that gender inequality shapes more than individuals’ careers. It also shapes the extent to which their ideas are used to create new technologies.},
	language = {en},
	number = {4059813},
	urldate = {2022-06-06},
	institution = {Social Science Research Network},
	author = {Bikard, Michaël and Fernandez-Mateo, Isabel},
	month = jan,
	year = {2022},
	doi = {10.2139/ssrn.4059813},
	keywords = {Technology, Science, Patents, Attention, Gender Inequality, Invention, Publications},
	file = {Full Text PDF:C\:\\Users\\jk2u1\\Zotero\\storage\\DEZUUYGP\\Bikard and Fernandez-Mateo - 2022 - Standing on the Shoulders of (Male) Giants Gender.pdf:application/pdf;Snapshot:C\:\\Users\\jk2u1\\Zotero\\storage\\L9IFA2SZ\\papers.html:text/html},
