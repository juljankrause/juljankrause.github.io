@incollection{zylaChapter25Burdensharing2023,
	title = {Chapter 25: {Burden}-sharing},
	isbn = {978-1-83910-339-1},
	shorttitle = {Chapter 25},
	url = {https://www.elgaronline.com/edcollchap/book/9781839103391/book-part-9781839103391-39.xml},
	abstract = {NATO burden-sharing has been a contentious issue for the Alliance since its inception in 1949. Debates have considered several input as well as output variables to determine burden-sharing fairness. The pertinent literature is rich, extensive and mainly of a rationalist flavour. However, it shows several gaps that future studies could take up and help fill. We know little about the ideational motivations of Allies to share burdens (or not), or about their deep-seated beliefs, values and principles that (may) guide their burden-sharing practices. Constructivist theorising has the potential to shed light on these questions and help elucidate state motivations, rather than simply treating them as an outcome. Especially practice theoretical approaches could help explain why a particular trend exists (e.g., European free-riding) and why it occurred at a particular point in time, as well as what value rational motivations officials have for - or against - sharing NATO burdens.},
