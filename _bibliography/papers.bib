	journal = {Systematic Reviews},
	author = {Gentles, Stephen J. and Charles, Cathy and Nicholas, David B. and Ploeg, Jenny and McKibbon, K. Ann},
	month = oct,
	year = {2016},
	pmid = {27729071},
	pmcid = {PMC5059917},
	file = {PubMed Central Full Text PDF:C\:\\Users\\jk2u1\\Zotero\\storage\\AVRCQHZ2\\Gentles et al. - 2016 - Reviewing the research methods literature princip.pdf:application/pdf},
