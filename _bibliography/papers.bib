@article{cartwrightPayNotGame2019,
	title = {To pay or not: game theoretic models of ransomware},
	volume = {5},
	issn = {2057-2085, 2057-2093},
	shorttitle = {To pay or not},
	url = {https://academic.oup.com/cybersecurity/article/doi/10.1093/cybsec/tyz009/5554879},
	doi = {10.1093/cybsec/tyz009},
	abstract = {Ransomware is a type of malware that encrypts ﬁles and demands a ransom from victims. It can be viewed as a form of kidnapping in which the criminal takes control of the victim’s ﬁles with the objective of ﬁnancial gain. In this article, we review and develop the game theoretic literature on kidnapping in order to gain insight on ransomware. The prior literature on kidnapping has largely focused on political or terrorist hostage taking. We demonstrate, however, that key models within the literature can be adapted to give critical new insight on ransomware. We primarily focus on two models. The ﬁrst gives insight on the optimal ransom that criminals should charge. The second gives insight on the role of deterrence through preventative measures. A key insight from both models will be the importance of spillover effects across victims. We will argue that such spillovers point to the need for some level of outside intervention, by governments or otherwise, to tackle ransomware.},
	language = {en},
	number = {1},
	urldate = {2020-07-23},
	journal = {Journal of Cybersecurity},
	author = {Cartwright, Edward and Hernandez Castro, Julio and Cartwright, Anna},
	month = jan,
	year = {2019},
	file = {Cartwright et al. - 2019 - To pay or not game theoretic models of ransomware.pdf:C\:\\Users\\jk2u1\\Zotero\\storage\\YJG4CI3C\\Cartwright et al. - 2019 - To pay or not game theoretic models of ransomware.pdf:application/pdf},
}

